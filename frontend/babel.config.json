{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "env": {
      "production": {
          "plugins": ["@babel/plugin-transform-object-rest-spread", "styled-jsx/babel", "@babel/plugin-syntax-jsx"]
      },
      "development": {
          "plugins": ["@babel/plugin-transform-object-rest-spread", "styled-jsx/babel", "@babel/plugin-syntax-jsx"]
      },
      "test": {
          "plugins": ["@babel/plugin-transform-object-rest-spread", "styled-jsx/babel-test"]
      }
  }
}